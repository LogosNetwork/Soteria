{
  "name": "full-node",
  "version": "0.0.1",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "test": "npm run unit && npm run e2e",
    "e2e": "npm run pack && nyc mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "dev": "node .electron-vue/dev-runner.js",
    "postinstall": "npm run lint:fix",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "unit": "karma start test/unit/karma.conf.js",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "@chenfengyuan/vue-qrcode": "^1.0.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-brands-svg-icons": "^5.10.2",
    "@fortawesome/pro-light-svg-icons": "^5.10.2",
    "@fortawesome/pro-solid-svg-icons": "^5.10.2",
    "@fortawesome/vue-fontawesome": "^0.1.7",
    "@logosnetwork/logos-webwallet-sdk": "^0.3.5",
    "axios": "^0.19.0",
    "big-integer": "^1.6.45",
    "bip39": "^2.6.0",
    "bootstrap-vue": "^2.0.1",
    "dayjs": "^1.8.16",
    "prismjs": "^1.17.1",
    "shelljs": "^0.8.3",
    "vue": "^2.6.10",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.14.1",
    "vue-input-autowidth": "^1.0.9",
    "vue-multiselect": "^2.1.6",
    "vue-prism-editor": "^0.3.0",
    "vue-qrcode-reader": "^2.0.4",
    "vue-router": "^3.1.3",
    "vue-virtual-scroll-list": "^1.4.2",
    "vuex": "^3.1.1",
    "vuex-electron": "git+https://github.com/MaverickMartyn/vuex-electron.git",
    "vuex-persist": "^2.1.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "@kazupon/vue-i18n-loader": "^0.4.1",
    "ajv": "^6.10.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "cfonts": "^2.4.5",
    "chai": "^4.1.2",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.0",
    "csp-html-webpack-plugin": "^3.0.3",
    "css-loader": "^3.2.0",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "^6.0.9",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "eslint": "^6.4.0",
