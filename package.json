{
  "name": "full-node",
  "version": "0.0.1",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "test": "npm run unit && npm run e2e",
    "e2e": "npm run pack && nyc mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "dev": "node .electron-vue/dev-runner.js",
    "postinstall": "npm run lint:fix",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "unit": "karma start test/unit/karma.conf.js",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/pro-light-svg-icons": "^5.8.2",
    "@fortawesome/pro-regular-svg-icons": "^5.8.2",
    "@fortawesome/pro-solid-svg-icons": "^5.8.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@logosnetwork/logos-rpc-client": "^1.1.29",
    "@logosnetwork/logos-webwallet-sdk": "^0.2.13",
    "axios": "^0.19.0",
    "bip39": "^2.6.0",
    "bootstrap-vue": "^2.0.0-rc.23",
    "vue": "^2.6.10",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.11.2",
    "vue-input-autowidth": "^1.0.5",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "vuex-electron": "git+https://github.com/MaverickMartyn/vuex-electron.git",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "ajv": "^6.10.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.1.4",
    "cfonts": "^2.4.3",
    "chai": "^4.1.2",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.1.6",
    "csp-html-webpack-plugin": "^3.0.2",
    "css-loader": "^3.0.0",
    "del": "^4.1.1",
    "devtron": "^1.4.0",
    "electron": "^5.0.3",
    "electron-builder": "^20.41.0",
    "electron-debug": "^3.0.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.4.1",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "popper.js": "^1.15.0",
    "require-dir": "^1.2.0",
    "sass-loader": "^7.1.0",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.0.0",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "author": "Tyler Storm <tyler@pla.bs>",
  "description": "Full Node with a UI that allows customization of logos node options as well as managing your logos accounts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LogosNetwork/Soteria.git"
  },
  "license": "UNLICENSED",
  "main": "./dist/electron/main.js",
  "build": {
    "productName": "Logos Node",
    "appId": "com.prometheanlabs.fullnode",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  }
}
